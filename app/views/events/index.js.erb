var average_events = <%= daily_average %>;
draw_graph("systems_chart","24 Hour Events by System",<%=raw system_stats(1) %>);
draw_graph("services_chart","24 Hour Events by Service",<%=raw service_stats(1) %>);
draw_gauge("7_days_chart", "7 Days", <%=raw total_events_since(7.days.ago) %>, 7);
draw_gauge("1_day_chart", "24 Hours", <%=raw total_events_since(24.hours.ago) %>, 1);
draw_gauge("1_hour_chart", "1 Hour", <%=raw total_events_since(1.hours.ago) %>, 1.0/24.0);
draw_diskspace_graph("diskutilization", "Disk Utilization", <%=raw disk_utilization() %>);
draw_longitudinal("logging_rate", "Event Logging Rate", "Events Per Minute", <%=raw events_per_minute(7.days.ago)%>);
draw_longitudinal("insert_rate", "Insert Rate", "Events Per Second", <%=raw inserts_per_second(1.hour.ago)%>);

var update_div = $("#updated")
update_div.fadeOut();
update_div.html("Last Updated: <%= Time.now %> | Pending Alerts: <%= Alert.where(:closed => false).count %>");
update_div.fadeIn();

setTimeout(function() {
	jQuery.ajax({
		type:'GET',
		url:'/events.js',
		dataType: 'script',
		complete: $.ajax()
		})}, 60000);